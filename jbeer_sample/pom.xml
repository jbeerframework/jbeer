			<groupId>com.jbeer.framework</groupId>
			<artifactId>jbeer_timer</artifactId>
			<version>0.1</version>
		</dependency>
		<dependency>
			<groupId>com.jbeer.framework</groupId>
			<artifactId>jbeer_ws</artifactId>
			<version>0.1</version>
		</dependency>
	</dependencies>
	<packaging>war</packaging>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>2.3.2</version>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
				</configuration>
			</plugin>
		</plugins>
