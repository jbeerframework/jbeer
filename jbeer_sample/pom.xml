			<artifactId>jbeer_server</artifactId>
			<version>0.1</version>
		</dependency>

		<dependency>
			<groupId>commons-logging</groupId>
			<artifactId>commons-logging-api</artifactId>
			<version>1.1</version>
		</dependency>
		<!-- <dependency>
			<groupId>com.jbeer.framework</groupId>
			<artifactId>jbeer_mybatis</artifactId>
			<version>0.1</version>
		</dependency> -->
